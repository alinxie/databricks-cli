{
  "name": "cli-example-lib-stack",
  "resources": [
    {
      "id": "example-library",
      "service": "dbfs",
      "properties": {
        "source_path": "lib/example_lib.jar",
        "path": "dbfs:/tmp/stack-cli-examples/example_lib.jar",
        "is_dir": false
      }
    },
    {
      "id": "example-job",
      "service": "jobs",
      "properties": {
        "name": "Stack CLI Example AWS Library Job",
        "libraries": [
          {
            "jar": "dbfs:/tmp/stack-cli-examples/example_lib.jar"
          }
        ],
        "new_cluster": {
          "spark_version": "4.0.x-scala2.11",
          "node_type_id": "r3.xlarge",
          "aws_attributes": {
            "availability": "SPOT"
          },
          "num_workers": 0
        },
        "spark_jar_task": {
          "main_class_name": "com.databricks.example.cli.HelloWorld"
        }
      }

    }
  ]
}
